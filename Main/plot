# Set the output to a png file
set terminal png size 600,600
# The file we'll write to
set output 'dens.png'
# The graphic title
set title 'Hartree-Potential'
set xrange[0.0:5.0];
set yrange[-5.0:5.0];
#plot the graphic
plot 'wfn.dat' u 1:2 w lp pt 7 ps 0.4 lc rgb "#FF3D00", \
     'wfn.dat' u 1:3 w lp pt 7 ps 0.4 lc rgb "#0000FF"

